<Window x:Class="SpaceInvaders.GameOver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvaders"
        mc:Ignorable="d"
        Title="GameOver" Height="300" Width="300">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="nebula.png"/>
        </Grid.Background>
        <Label x:Name="label" Content="GAME OVER" Height="43" Margin="47,23,50.6,0" Width="196" FontSize="24" FontStyle="Italic" FontWeight="Bold" Foreground="#FF00FFF1" BorderBrush="#FF00FFF1" Background="{x:Null}" HorizontalContentAlignment="Center" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Press any key to restart" Height="43" Margin="10,99,9.6,0" Width="274" FontSize="24" FontStyle="Italic" FontWeight="Bold" Foreground="#FF00FFF1" BorderBrush="#FF00FFF1" Background="{x:Null}" VerticalAlignment="Top" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
